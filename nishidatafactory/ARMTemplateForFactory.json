{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "nishidatafactory"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"newblob_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/c1e281f0-a0f6-4c5f-922c-10604ebc543e/resourceGroups/rs/providers/Microsoft.Storage/storageAccounts/nishistorageaccoubt"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Wait1",
						"type": "Wait",
						"dependsOn": [],
						"userProperties": [],
						"typeProperties": {
							"waitTimeInSeconds": 1
						}
					}
				],
				"annotations": []
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/DelimitedText1')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureBlobStorage1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "Hackathon_Dish_Care.csv",
						"container": "demo"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "Markets",
						"type": "String"
					},
					{
						"name": "PG MEGACATEGORY",
						"type": "String"
					},
					{
						"name": "PG SUPER CATEGORY",
						"type": "String"
					},
					{
						"name": "PG CATEGORY",
						"type": "String"
					},
					{
						"name": "PG SEGMENT",
						"type": "String"
					},
					{
						"name": "PG FORM",
						"type": "String"
					},
					{
						"name": "PG MANUFACTURER",
						"type": "String"
					},
					{
						"name": "PG BRAND",
						"type": "String"
					},
					{
						"name": "PG SUBBRAND",
						"type": "String"
					},
					{
						"name": "PG SIZE",
						"type": "String"
					},
					{
						"name": "PG VARIETY",
						"type": "String"
					},
					{
						"name": "PG CONCENTRATION",
						"type": "String"
					},
					{
						"name": "PG SCENT GROUP",
						"type": "String"
					},
					{
						"name": "PG PRICE SEGMENT",
						"type": "String"
					},
					{
						"name": "PG SKU DETAIL",
						"type": "String"
					},
					{
						"name": "Periods",
						"type": "String"
					},
					{
						"name": "$0 ",
						"type": "String"
					},
					{
						"name": "VOL (000)",
						"type": "String"
					},
					{
						"name": "TDP",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureBlobStorage1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlobStorage1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('AzureBlobStorage1_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/newblob')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Stopped",
				"pipelines": [],
				"type": "BlobEventsTrigger",
				"typeProperties": {
					"blobPathBeginsWith": "/input/blobs/",
					"ignoreEmptyBlobs": true,
					"scope": "[parameters('newblob_properties_typeProperties_scope')]",
					"events": [
						"Microsoft.Storage.BlobCreated"
					]
				}
			},
			"dependsOn": []
		}
	]
}